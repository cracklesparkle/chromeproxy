{
    "manifest_version": 3,
    "name": "Umbrella Proxy",
    "version": "1.0",
    "permissions": [
        "proxy",
        "storage",
        "tabs",
        "webNavigation"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "host_permissions": [
        "<all_urls>"
    ]
}